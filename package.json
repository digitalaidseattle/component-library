{
  "name": "@digitalaidseattle/root",
  "version": "1.0.1",
  "description": "A library of common components for DAS ventures.",
  "repository": {
    "type": "git",
    "url": "null//github.com/null/github.com"
  },
  "author": "Misty Becker <misty.becker@digitalaidseattle.org>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "build": "preconstruct build",
    "release": "yarn build && changeset publish",
    "test": "jest",
    "clean": "manypkg exec rm -rf node_modules && manypkg exec rm -rf dist && rm -rf node_modules",
    "lint": "yarn eslint ."
  },
  "dependencies": {
    "@babel/core": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.25.9",
    "@changesets/cli": "^2.27.9",
    "@manypkg/cli": "^0.22.0",
    "@preconstruct/cli": "^2.8.9",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.2.4",
    "vite": "^5.4.10",
    "vitest": "^1.2.2"
  },
  "preconstruct": {
    "apps": [
      "apps/*"
    ],
    "packages": [
      "packages/*"
    ]
  }
