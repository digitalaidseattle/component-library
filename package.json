{
  "name": "@digitalaidseattle/root",
  "version": "1.0.0",
  "description": "A library of common components for DAS ventures.",
  "repository": "git@github.com:digitalaidseattle/component-library.git",
  "author": "Misty Becker <misty.becker@digitalaidseattle.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "build": "preconstruct build",
    "release": "yarn build && changeset publish",
    "test": "jest",
    "clean": "manypkg exec rm -rf node_modules && manypkg exec rm -rf dist && rm -rf node_modules",
    "lint": "yarn eslint ."
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@changesets/cli": "^2.5.0",
    "@manypkg/cli": "^0.10.0",
    "@preconstruct/cli": "^1.1.4",
    "@types/jest": "^29.5.12",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^29.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^1.19.1",
    "ts-jest": "^29.2.4"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
